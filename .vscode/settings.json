{
    "cSpell.words": [
        "GPFS",
        "MMLSFS"
    ],

    "cSpell.languageSettings": [
        // This one works with python
        {
            "languageId": "python",
            "includeRegExpList": [
                "/#.*/",
                "/('''|\"\"\")[^\\1]+?\\1/g"
            ]
        }
    ],

    "files.trimTrailingWhitespace": true,

    "python.linting.pylintEnabled": true,
    "python.linting.pycodestyleEnabled": false
}